@model IEnumerable<Csv>
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<form asp-controller="Csv" asp-action="Index" method="post" enctype="multipart/form-data">
    <input type="file" name="file" />
    <hr />
    <button type="submit">Upload</button>
</form>

<div id="PlaceHolderHere"></div>


<div class="form-group">
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Email)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.StartDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.EndDate)
                </th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Email)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.StartDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EndDate)
                    </td>
                    <td class="form-group">
                        <button type="button" class="btn btn-primary" data-toggle="ajax-modal"
                            data-target="#addCsv" data-url="/Csv/Index?id=@item.MemberId">
                            Edit
                        </button> |

                        @Html.ActionLink("Delete", "Delete", new { id = item.MemberId },
                    new
                    {
                    @class = "btn btn-primary",
                    data_url = "/Csv/Delete",
                    onclick = "return confirm('Are you sure you want to delete?')"
                    })
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
