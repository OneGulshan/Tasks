@model DataAccessLayer.Models.Csv

@{
    <form asp-action="Index" asp-controller="Csv" method="post">
    @Html.HiddenFor(m=>m.MemberId)
        <div class="form-group">
            <div class="modal fade" id="addCsv">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h4 class="modal-title">Add Csv</h4>
                            <button type="button" class="close" data-bs-dismiss="modal">
                                <span>x</span>
                            </button>
                        </div>

                        <div class="modal-body">

                            <div class="mb-3">
                                <label asp-for="Email"></label>
                                <input asp-for="Email" class="form-control" />
                                <span asp-validation-for="Email" class="text-danger"></span>
                            </div>
                            <div class="mb-3">
                                <label asp-for="StartDate"></label>
                                <input asp-for="StartDate" class="form-control" />
                                <span asp-validation-for="StartDate" class="text-danger"></span>
                            </div>
                            <div class="mb-3">
                                <label asp-for="EndDate"></label>
                                <input asp-for="EndDate" class="form-control" />
                                <span asp-validation-for="EndDate" class="text-danger"></span>
                            </div>

                            <button type="button" class="btn btn-primary" data-bs-dismiss="modal">Close</button>
                            <input type="submit" value="@ViewBag.BT" class="btn btn-primary" />
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>
}